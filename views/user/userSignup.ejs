	<script>
		const userNameError = document.getElementById('usernameError');
		const emailError = document.getElementById('emailError');
		const numberError = document.getElementById('numberError');
		const passError = document.getElementById('passError');
		const conpass = document.getElementById('conpassError');

		function validateUsername() {
			const username = document.getElementById('name').value;
			console.log(username)
			if (username.length == 0) {
				userNameError.innerHTML = 'Enter the username'
				return false
			}
			if (!username.match(/^[a-zA-Z][a-zA-Z_-]{4,19}$/)) {
				userNameError.innerHTML = 'shlould not contain any numbers and length should be morethan 5'
				return false
			}
			userNameError.innerHTML = ''
			return true
		}
		function validatEmail() {
			const email = document.getElementById('email').value;
			console.log(emailError)
			if (email.length == 0) {
				emailError.innerHTML = 'enter the Email'
				return false
			}
			if (!email.match(/^[\w.-]+@[a-zA-Z\d.-]+\.[a-zA-Z]{2,}$/)) {
				emailError.innerHTML = 'enter valid email'
				return false;
			}
			emailError.innerHTML = ''
			return true
		}

		function validatePassword() {
			const password = document.getElementById('pass').value;
			console.log(passError);
			if (password.length == 0) {
				passError.innerHTML = 'enter the Password'
				return false;

			}
			if (!password.match(/^[A-Za-z0-9]{8,}/)) {
				passError.innerHTML = 'password should have min 8 character'
				return false;
			}
			passError.innerHTML = ''
			return true;
		}

		function validateConfpass() {
			const confirm = document.getElementById('conpass').value;
			const password = document.getElementById('pass').value;
			console.log(conpass);
			if (confirm != password) {
				conpass.innerHTML = 'password should match'
				return false
			}
			conpass.innerHTML = '';
			return true;

		}

		function validateForm() {
			if (!validateUsername() || !validatEmail() || !validatePassword() || !validateConfpass()) {
				submitError.style.display = 'block'
				submitError.innerHTML = 'Correct the mistake in the form'
				setTimeout(() => {
					submitError.style.display = 'none'
				}, 4000)
				return false
			}
			return true;
		}


	</script>
